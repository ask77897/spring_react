<mapper namespace="com.example.mapper.PurchaseMapper">
    <insert id="inpurchase">
    	insert into purchase(oid, uid, uname, phone, address1, address2, sum)
    	values(#{oid}, #{uid}, #{uname}, #{phone}, #{address1}, #{address2}, #{sum})
    </insert>
</mapper>